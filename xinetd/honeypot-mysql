# Container launcher for honeypot
service honeypot-mysql
{
        disable         = no
	type		= UNLISTED
        instances       = UNLIMITED
        server          = /usr/local/bin/honeypot-mysql
        socket_type     = stream
        protocol        = tcp
        port            = 3306
        user            = root
        wait            = no
        log_type        = SYSLOG authpriv info
        log_on_success  = HOST PID
        log_on_failure  = HOST
}
